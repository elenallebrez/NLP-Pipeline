        "def generar_embedding(texto):\n",
        "    try:\n",
        "        response = genai.embed_content(\n",
        "            model=EMBEDDING_MODEL,\n",
        "            content=texto,\n",
        "            task_type=\"semantic_similarity\",  # puedes cambiarlo segÃºn necesidad\n",
        "        )\n",
        "        return response['embedding']\n",
        "    except Exception as e:\n",
        "        print(f\"âŒ Error con Gemini embedding: {e}\")\n",
        "        return None\n",
        "\n",
        "# âœ… Mostrar informaciÃ³n\n",
        "print(\"ğŸ“ Carpeta de entrada:\", os.path.abspath(folder_path))\n",
        "\n",
        "try:\n",
        "    files = sorted(os.listdir(folder_path))[:10]\n",
        "except FileNotFoundError:\n",
        "    print(f\"âŒ La carpeta {folder_path} no existe.\")\n",
        "    exit()\n",
        "\n",
        "print(\"ğŸ“„ Archivos detectados:\", files)\n",
        "\n",
        "# âœ… Procesar archivos\n",
        "for name_file in files:\n",
        "    ruta = os.path.join(folder_path, name_file)\n",
        "\n",
        "    if not name_file.endswith('.json'):\n",
        "        continue\n",
        "\n",
        "    if not os.path.isfile(ruta):\n",
        "        print(f\"âš ï¸ Archivo no encontrado (saltado): {ruta}\")\n",
        "        continue\n",
        "\n",
        "    with open(ruta, 'r', encoding='utf-8') as f:\n",
        "        try:\n",
        "            data = json.load(f)\n",
        "        except Exception as e:\n",
        "            print(f\"âŒ Error leyendo {name_file}: {e}\")\n",
        "            continue\n",
        "\n",
        "    textos = []\n",
        "\n",
        "    # âœ… Extraer los textos de los mensajes\n",
        "    if isinstance(data, dict) and 'messages' in data:\n",
        "        textos = [msg['content'] for msg in data['messages']\n",
        "                  if isinstance(msg, dict) and 'content' in msg and isinstance(msg['content'], str)]\n",
        "\n",
        "    if not textos:\n",
        "        print(f\"âš ï¸ No se encontraron textos en {name_file}\")\n",
        "        continue\n",
        "\n",
        "    embeddings = []\n",
        "    for t in textos:\n",
        "        emb = generar_embedding(t)\n",
        "        if emb:\n",
        "            embeddings.append(emb)\n",
        "\n",
        "    if not embeddings:\n",
        "        print(f\"âš ï¸ No se generaron embeddings en {name_file}\")\n",
        "        continue\n",
        "\n",
        "    nombre_salida = os.path.splitext(name_file)[0] + \"_embeddings.json\"\n",
        "    ruta_salida = os.path.join(out_path, nombre_salida)\n",
        "\n",
        "    with open(ruta_salida, 'w', encoding='utf-8') as f:\n",
        "        json.dump(embeddings, f)\n",
        "\n",
        "    print(f\"âœ… Procesado y guardado: {ruta_salida} ({len(embeddings)} vectores)\")"
